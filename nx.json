{
  "taskRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default"
    },
    "options": {
      "cacheableOperations": [
        "build"
      ]
    }
  },
  "namedInputs": {
    "noMarkdown": [
      "{projectRoot}/**/*.md"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "{projectRoot}/**/*.md"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "inputs": [
        "noMarkdown"
      ]
    }
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "my-new-app"
}
